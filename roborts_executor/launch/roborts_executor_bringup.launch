<?xml version="1.0" encoding="utf-8" ?>
<launch>
    <node pkg="roborts_executor"
          name="executor_action_node"
          type="executor_action_node"
          output="screen"
          required="true">
        <rosparam command="load" file="$(find roborts_executor)/param/executor_action.yaml"/>
    </node>

    <node pkg="dynamic_reconfigure"
          name="dynparam"
          type="dynparam"
          args="load executor_action_node $(find roborts_executor)/param/executor_action.yaml">

    </node>

</launch>
